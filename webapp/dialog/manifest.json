{
	"_version": "1.1.0",

	"sap.app": {
		"_version": "1.1.0",
		"id": "sap.training.dialog",
		"type": "component",
		"i18n": "i18n/i18n.properties",
		"title": "{{compTitle}}",
		"description": "{{compDescription}}",
		"applicationVersion": {
			"version": "0.1.0"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "sap.training.dialog.view.Main",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {
					"minVersion": "1.30"
				}
			}
		},
		"config": {
			"displayDialog": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.training.dialog.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	}
}